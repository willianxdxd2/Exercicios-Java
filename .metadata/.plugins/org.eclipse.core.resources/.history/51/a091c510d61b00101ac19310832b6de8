		SimpleDateFormat sdf = new SimpleDateFormat("DD/MM/YYYY");
		LocalDate momentDate = LocalDate.now();
		String dateFormated = sdf.format(momentDate);
		System.out.println(momentDate);
		
		
		
		
	}

}
